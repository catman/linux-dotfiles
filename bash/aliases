# hadoop
		# In Hadoop's config file:
		#   /usr/local/Cellar/hadoop/1.2.1/libexec/conf/hadoop-env.sh
		# $JAVA_HOME has been set to be the output of:
		#   /usr/libexec/java_home
alias had_cd='cd /usr/local/Cellar/hadoop/1.2.1/bin'
alias had_start='/usr/local/Cellar/hadoop/1.2.1/bin/start-all.sh'
alias had_stop='/usr/local/Cellar/hadoop/1.2.1/bin/stop-all.sh'
alias had_log='tail -f /usr/local/Cellar/hadoop/1.2.1/libexec/logs/hadoop-david-datanode-rab-macmini.log'
alias had_format='hadoop namenode -format'
alias had_test='hadoop jar /usr/local/Cellar/hadoop/1.2.1/libexec/hadoop-examples-1.2.1.jar pi 10 100'


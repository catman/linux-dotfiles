function fstartup() {
	
	echo "-- in /Library/LaunchDaemons.."
	ls /Library/LaunchDaemons | grep $1
	
	echo "-- in /System/Library/LaunchDaemons .."
	ls /System/Library/LaunchDaemons | grep $1
	
	echo "-- in /Library/LaunchAgents .."
	ls /Library/LaunchAgents | grep $1
	
	echo "-- in /System/Library/LaunchAgents .."
	ls /System/Library/LaunchAgents | grep $1
	
	echo "-- in /Users/david/Library/LaunchAgents .."
	ls /Users/david/Library/LaunchAgents | grep $1
}



